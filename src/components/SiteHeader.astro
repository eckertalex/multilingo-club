---
import SkipNav from "./SkipNav.astro";
import SiteLogo from "./SiteLogo.astro";
import siteConfig from "../content/siteConfig.json";
---

<header class="flex items-center justify-between py-4 mb-8">
  <SkipNav />
  <SiteLogo />
  <nav class="flex gap-8">
    {
      siteConfig.navigation.map((item) => (
        <a href={item.slug} class="text-lg/7 font-semibold text-gray-900">
          {item.label}
        </a>
      ))
    }
  </nav>
</header>
